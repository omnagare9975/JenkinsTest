{
    "family": "nodejs-app-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "nodejs-app",
        "image": "<IMAGE_URI>",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "executionRoleArn": "arn:aws:iam::108782100108:role/ecsTaskExecutionRole"
  }
  